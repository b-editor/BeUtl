<Styles
    xmlns="https://github.com/avaloniaui"
    xmlns:sys="using:System"
    xmlns:sty="using:FluentAvalonia.Styling"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Styles.Resources>
        <SolidColorBrush x:Key="LayerStrongBorderBrush" Color="#28FFFFFF" />
        <SolidColorBrush x:Key="LayerThinBorderBrush" Color="#18FFFFFF" />
    </Styles.Resources>

    <FluentTheme Mode="Dark" />
    <sty:FluentAvaloniaTheme
        ControlsVersion="2"
        DefaultToUserTheme="False"
        GetUserAccentColor="True"
        RequestedTheme="Dark" />
</Styles>
