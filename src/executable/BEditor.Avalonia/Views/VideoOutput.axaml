<views:FluentWindow
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:titlebar="using:BEditor.Views.CustomTitlebars"
    xmlns:prop="using:BEditor.LangResources"
    xmlns:views="using:BEditor.Views"
    xmlns:vm="using:BEditor.ViewModels"
    xmlns:ui="using:FluentAvalonia.UI.Controls"
    mc:Ignorable="d"
    x:DataType="vm:VideoOutputViewModel"
    Height="800" Width="1200"
    WindowStartupLocation="CenterScreen"
    x:Class="BEditor.Views.VideoOutput">
    <ui:NavigationView
        PaneDisplayMode="Left"
        x:Name="NavView"
        IsSettingsVisible="False">
        <ui:Frame x:Name="FrameView" />
    </ui:NavigationView>
</views:FluentWindow>
